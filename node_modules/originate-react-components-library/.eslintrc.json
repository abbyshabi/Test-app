{
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "react-app"
    ],
    "plugins": [
        "react",
        "prettier"
    ],
    "env": {
        "browser": true,
        "jasmine": true,
        "jest": true
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true
            }
        ]
    }
}